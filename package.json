    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.7.1",
    "vouchdao": "0.2.6",
    "warp-contracts": "1.4.8"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "arlocal": "1.1.60",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "prettier": "2.8.4",
    "typescript": "4.7.4"
